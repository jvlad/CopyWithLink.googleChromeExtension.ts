{
  "manifest_version": 2,

  "name": "Copy With Link",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.ico"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/vendor.js"]
      }
  ],

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"]
  },

  "permissions": [
    "activeTab"
  ]
}